{
  "name": "ogl",
  "version": "1.0.0",
  "description": "OGL Vanilla TypeScript Template",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "concurrently --names \"ROLLUP,HTTP\" -c \"bgBlue.bold,bgGreen.bold\" \"rollup -c -w -m inline\" \"servez public\"",
    "start": "servez public",
    "build": "rollup -c"
  },
  "dependencies": {
    "ogl": "^1.0.8"
  },
  "devDependencies": {
    "@rollup/plugin-node-resolve": "^15.3.0",
    "@rollup/plugin-typescript": "^12.1.0",
    "concurrently": "^9.0.1",
    "prettier": "^3.3.3",
    "rollup": "^4.24.0",
    "servez": "^2.2.4",
    "tslib": "^2.7.0",
    "typescript": "^5.6.3"
  },
  "prettier": {
    "arrowParens": "always",
    "bracketSpacing": true,
    "endOfLine": "lf",
    "htmlWhitespaceSensitivity": "css",
    "printWidth": 200,
    "quoteProps": "as-needed",
    "semi": true,
    "singleQuote": true,
    "tabWidth": 4,
    "trailingComma": "es5",
    "useTabs": false
  }
}
